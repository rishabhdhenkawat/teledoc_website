<body>

    <!-- connects to the server and displays results -->
    <h1 class="h3 mb-3 font-weight-normal">Chat With Me</h1>
    <div class="container" id="chat">
        <div id="chat-window">
            <div id="output"></div>
              <div id="typing"></div>
        </div>

        <div class="container">

          <input id="handle" type="text" class="form-control" placeholder="name" required autofocus>
          <input id="message" type="text" class="form-control" placeholder="message" required>
          <button id="button" class="btn btn-lg btn-primary btn-block" type="submit">Send</button>
          
        </div>
        
      <hr>
      
      <div class="container">
        <div class="row">
          <div class="col-sm-6 col-md-6 col-lg-6">
            remote
            <video id="rVideo" autoplay="autoplay"> 
          </div>
          <div class="col-sm">
            local
            <video id="lVideo" autoplay="autoplay" muted>
          </div>
        </div>
        <span id="displayId"></span>
        <input type="text" id="connId" class="form-control" placeholder="enter a connection id" required>
        <button id="conn_button" class="btn btn-lg btn-success btn-block" type="submit">Connect</button>
        <button id="call_button" class="btn btn-lg btn-danger btn-block" type="submit">Call</button>
      </div>  

    </div>


    <!-- <script src="http://localhost:3000/socket.io/socket.io.js" async defer></script> -->

    <!-- <script src="socket.io/socket.io.js"></script>  -->
